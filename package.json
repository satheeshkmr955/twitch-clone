{
  "name": "twitch-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev-all": "concurrently \"npm:codegen-watch\" \"next dev\"",
    "dev": "next dev",
    "dev-tsnode": "nodemon app/api/graphql/server.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen-watch": "graphql-codegen --watch --config codegen.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --skip-seed",
    "db:reset": "prisma migrate reset --skip-seed",
    "db:studio": "prisma studio"
  },
